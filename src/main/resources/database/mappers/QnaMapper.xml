<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
    <mapper namespace="com.fd.s1.board.qna.QnaMapper">
    	
    	<select id="getQnaList" resultType="QnaVO">
    	SELECT * FROM QNA
    	</select>
    	<select id="getQnaDetail" parameterType="QnaVO" resultType="QnaVO">
    	SELECT * FROM QNA WHERE NUM=#{num}
    	</select>
		<insert id="setQnaAdd" parameterType="QnaVO" useGeneratedKeys="true" keyProperty="num">
		INSERT INTO QNA VALUES (#{num},#{id},#{location},#{kind},#{email},#{phone},#{title},#{contents},now(),0,#{num},0,0)
		</insert>
    	
    </mapper>
